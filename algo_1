array = [3,5 -4,8, 11, 1 ,-1, 6]
targetSum = 10
l=[]
def algo1(array,targetSum):
    for each in array:
        number=targetSum-each
        if number in array:
            l.append(each)
            l.append(number)
            break
    return l

if __name__=="__main__":
    array = [3,5 -4,8, 11, 1 ,-1, 6]
    targetSum = 10
    print(algo1(array,targetSum))